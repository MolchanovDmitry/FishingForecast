CREATE TABLE MapPoint(
    id TEXT NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    profileName TEXT,
    latitude REAL NOT NULL,
    longitude REAL NOT NULL,
    FOREIGN KEY(profileName) REFERENCES Profile(name)
);

selectAll:
SELECT *
FROM MapPoint;

select:
SELECT *
FROM MapPoint
WHERE id = ?;

insert:
INSERT INTO MapPoint(id, name, profileName, latitude, longitude)
VALUES ?;