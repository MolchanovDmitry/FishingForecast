CREATE TABLE profile(
    name TEXT NOT NULL PRIMARY KEY
);

selectAll:
SELECT *
FROM profile;

insert:
INSERT INTO profile(name)
VALUES ?;

DELETE FROM profile WHERE ?;

delete:
DELETE FROM profile
WHERE name = ?;

